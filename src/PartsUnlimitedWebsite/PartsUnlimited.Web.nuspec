<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>PartsUnlimited.Web</id>
    <title>PartsUnlimited.Web</title>
    <version>$version$</version>
    <authors>Centare</authors>
    <owners>Centare</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Parts Unlimited Web Application Package</description>
  </metadata>
  <files>
    <file src="obj\$Configuration$\Package\PackageTmp\**\*.*" target="" />
    <file src="*Dev.config" target="" />
    <file src="*Qa.config" target="" />
    <file src="*Uat.config" target="" />
    <file src="*Prod.config" target="" />
    <!-- Packaging for web jobs -->
    <file src="..\PartsUnlimited.WebJob.ProcessOrder\bin\$Configuration$\**\*.*" target="app_data\jobs\continuous\ProcessOrder" />
    <file src="..\PartsUnlimited.WebJobs.UpdateProductInventory\bin\$Configuration$\**\*.*" target="app_data\jobs\triggered\UpdateProductInventory" />
  </files>
</package>